local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local PhantomForcesWindow = Library:NewWindow("ðŸ˜¡ BOSSJKLO EXPLOIT ðŸ˜¡")

local Bladeball = PhantomForcesWindow:NewSection("BladeBall MainTab V1")

Bladeball:CreateToggle("SPEED WALK ðŸ˜¡", function()
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 53
game.Players.LocalPlayer.Character:WaitForChild("Humanoid").WalkSpeed = 53
end)

Bladeball:CreateToggle("Auto-Kill Bosses ðŸ˜¡", function()
Callback = function(bool)
      getgenv().autokillboss = bool
		while getgenv().autokillboss == true and boss() ~= "none" do
         local punch = boss()
         task.wait(.1)
         local args = {
         [1] = workspace:WaitForChild(punch),
         [2] = Vector3.new(workspace:WaitForChild(punch):WaitForChild("Right Leg").Position),
         [3] = 1.8757749795913696,
         [4] = workspace:WaitForChild(punch):WaitForChild("Right Leg")
      }
      game:GetService("ReplicatedStorage"):WaitForChild("Remote Events"):WaitForChild("Punch"):FireServer(unpack(args))
      end
	end
end)

Bladeball:CreateToggle("TELEPORT ðŸ˜¡", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Infinity2346/Tect-Menu/main/Teleport%20Gui.lua"))()
end)

Bladeball:CreateButton("Punch-Aura ðŸ˜¡", function()
getgenv().aurareach = 19
	end
end)

Bladeball:CreateToggle("EXPLOIT JOINED ðŸ˜¡",
function()
loadstring(game:HttpGet("https://pastebin.com/raw/dS6nMuAJ"))()
end)
